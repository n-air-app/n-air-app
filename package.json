{
  "name": "n-air-app",
  "description": "Streaming application for niconico",
  "author": "DWANGO Co.,Ltd.",
  "license": "GPL-3.0",
  "//": {
    "version": "0.11.2"
  },
  "version": "1.1.20240403-unstable.1",
  "main": "main.js",
  "engines": {
    "npm": "please-use-yarn"
  },
  "browserslist": [
    "chrome >= 76"
  ],
  "scripts": {
    "compile": "yarn clear && webpack-cli --progress --mode development",
    "compile:ci": "yarn clear && webpack-cli --mode development",
    "compile:production": "yarn clear && webpack-cli --progress --mode production",
    "watch": "yarn clear && webpack-cli --watch --progress --mode development",
    "start": "node bin/start-with-auto-env.js",
    "dev:server": "webpack-cli serve --progress --hot --mode development",
    "dev:start": "cross-env NAIR_UNSTABLE=1 DEV_SERVER=\"http://localhost:8080/index.dev.html\" electron . && echo 'electron exited.'",
    "dev": "run-p dev:*",
    "start:stable": "electron .",
    "start:unstable": "cross-env NAIR_UNSTABLE=1 electron .",
    "clear-plugins": "shx rm -rf plugins",
    "package": "yarn package:public-stable",
    "package:public-stable": "shx rm -rf dist && yarn install --cwd bin && node bin/convert-to-shiftjis.js AGREEMENT && electron-builder build -w --x64 --config electron-builder/stable.config.js",
    "package:public-unstable": "shx rm -rf dist && yarn install --cwd bin && node bin/convert-to-shiftjis.js AGREEMENT && electron-builder build -w --x64 --config electron-builder/unstable.config.js",
    "package:internal-stable": "shx rm -rf dist && yarn install --cwd bin && node bin/convert-to-shiftjis.js AGREEMENT && electron-builder build -w --x64 --config electron-builder/internal-stable.config.js",
    "package:internal-unstable": "shx rm -rf dist && yarn install --cwd bin && node bin/convert-to-shiftjis.js AGREEMENT && electron-builder build -w --x64 --config electron-builder/internal-unstable.config.js",
    "package:local": "shx rm -rf dist && yarn install --cwd bin && node bin/convert-to-shiftjis.js AGREEMENT && electron-builder build -w --x64 --config electron-builder/local.config.js",
    "release": "yarn install --cwd bin && node bin/release/mini-release.js",
    "patch-note": "yarn install --cwd bin && node bin/release/generatePatchNote.js",
    "webfont": "yarn install --cwd bin && node bin/generate-webfont.js",
    "eslint": "eslint app/**/*.ts",
    "test": "node bin/i18n-early-check.js && tsc -p test && ava -v -s",
    "test:unit": "yarn test:unit:app && yarn test:unit:bin",
    "test:unit:app": "jest --silent --config ./jest.config.js",
    "test:unit:bin": "jest --silent --config ./jest.bin-config.js",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css,less} --ignore-path .gitignore .",
    "lint": "eslint \"{app,guest-api,obs-api,updater}/**/*.ts\" main.js&&yarn lint:js&&yarn lint:style",
    "clear": "shx rm -rf bundles/media",
    "typedoc": "typedoc --out docs/dist ./app/services --mode modules --theme ./docs/theme --readme ./docs/README.md --ignoreCompilerErrors --excludePrivate --excludeExternals --hideGenerator",
    "compile-tests": "tsc -p test",
    "screentest": "node test/screentest/runner.js",
    "version": "git add app/services/patch-notes/notes.ts",
    "postinstall": "node --no-warnings ./scripts/install-native-deps.js",
    "clear:cache": "npx shx rm -rf node_module/.cache dist bundles test-dist",
    "prettier": "eslint --fix && prettier -w ./app/**/*.{ts,vue,tsx}"
  },
  "buildProductName": "N Air",
  "ava": {
    "files": [
      "test-dist/test/e2e/*.js",
      "test-dist/test/api/*.js"
    ],
    "timeout": "3min"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf",
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less,vue}": [
      "stylelint --fix",
      "git add"
    ],
    "app/i18n/*/*.json": [
      "node ./bin/i18n-early-check.js"
    ],
    "{app/fonts/glyphs/*.svg,app/styles/custom-icons.less.njk}": [
      "yarn webfont",
      "git add app/fonts/n-air.woff app/styles/custom-icons.less"
    ]
  },
